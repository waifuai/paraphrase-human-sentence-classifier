# Human vs Machine Sentence Classifier Configuration
# Copy this file to .env and fill in your values

# API Keys (choose one for Gemini)
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# OpenRouter API Key
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Model Selection (optional, defaults will be used if not specified)
GEMINI_MODEL=gemini-2.5-pro
OPENROUTER_MODEL=deepseek/deepseek-r1-0528:free

# Default Provider (gemini or openrouter)
DEFAULT_PROVIDER=openrouter

# Evaluation Settings
EVALUATION_DELAY=0.1
OUTPUT_DIR=results

# Logging
LOG_LEVEL=INFO

# Retry Settings
GEMINI_MAX_RETRIES=3
OPENROUTER_MAX_RETRIES=3